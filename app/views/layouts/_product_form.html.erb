<%= render "layouts/errors", :object => @product %>

<%= form_for @product do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %> <br>
  <%= f.label :Image_url%>
  <%= f.text_field :img %> <br>
  <%= f.label :Cost %>
  <%= f.text_field :cost %> <br>
  <%= f.label :country_of_origin %>
  <%= f.text_field :country_of_origin %> <br>
  <br>
  <%= f.submit %>
<% end %>
<br>
<h4><%= link_to "Return to Treats", products_path %></h4>
